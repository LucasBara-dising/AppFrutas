<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ImgcardHome">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">74dp</item>
        <item name="android:layout_margin">18dp</item>
    </style>

    <style name="TxtcardHome">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">94dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="TxtBaseHome">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">14dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">@font/alata</item>
        <item name="android:textColor">@color/black</item>
    </style>
</resources>